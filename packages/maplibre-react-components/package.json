    "postcss-nesting": "^13.0.1",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "storybook": "^8.6.14",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "catalog:",
